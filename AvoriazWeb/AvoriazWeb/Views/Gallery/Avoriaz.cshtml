@{
    ViewBag.Title = "Gallery";
}
<h2>@ViewBag.Title</h2>

<h4>@ViewBag.Message</h4>

@Html.Partial("../Avoriaz/navBarAvoriaz")

<div class="gallery">

    @foreach (var imgFile in ViewBag.imageArray)
    {
        <a href="/Content/Images/Avoriaz/@Path.GetFileName(imgFile)" data-lightbox="mygallery" data-title="Avoriaz Image"> <img src="/Content/Images/Avoriaz/@Path.GetFileName(imgFile)" alt="Avoriaz image" /> </a>
    }

</div>


