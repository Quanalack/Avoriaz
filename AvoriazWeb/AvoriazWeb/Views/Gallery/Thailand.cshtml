@{
    ViewBag.Title = "Gallery";
}
<h2>@ViewBag.Title</h2>

<h4>@ViewBag.Message</h4>

@Html.Partial("../Thailand/navBarThailand")

<div class="gallery">

    @foreach (var imgFile in ViewBag.imageArray)
    {
    <a href="~/Content/Images/thailand/@Path.GetFileName(imgFile)" data-lightbox="mygallery" data-title="Thailand Image"> <img src="/Content/Images/Thailand/@Path.GetFileName(imgFile)" alt="Thailand image" /> </a>
    }

</div>


